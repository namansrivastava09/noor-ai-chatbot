rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // This allows listing documents in the messages collection for the specific user.
    match /users/katyayani_uid/messages {
      allow list: if true;
    }
    // This allows reading and writing individual documents in that collection.
    match /users/katyayani_uid/messages/{messageId} {
      allow get, write: if true;
    }
  }
}